{
    "name": "poltak.github.io",
    "version": "0.0.2",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "deploy:raw": "gh-pages -d build",
        "deploy": "npm run build && npm run deploy:raw",
        "test": "vitest",
        "test:single": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "lint": "prettier --check .",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^4.0.0",
        "@sveltejs/adapter-static": "^3.0.8",
        "@sveltejs/kit": "^2.16.1",
        "@sveltejs/vite-plugin-svelte": "^5.0.3",
        "@types/seedrandom": "^3.0.8",
        "@vitest/coverage-v8": "^3.0.4",
        "@vitest/ui": "^3.0.4",
        "gh-pages": "^6.3.0",
        "jsdom": "^26.1.0",
        "mdsvex": "^0.12.3",
        "prettier": "^3.4.2",
        "prettier-plugin-svelte": "^3.3.3",
        "svelte": "^5.19.3",
        "svelte-check": "^4.1.4",
        "tslib": "^2.8.1",
        "typescript": "^5.7.3",
        "vite": "^6.0.11",
        "vitest": "^3.0.4"
    },
    "dependencies": {
        "@zip.js/zip.js": "^2.7.62",
        "kindle-highlights-parser": "^0.1.1",
        "minisearch": "^6.3.0",
        "seedrandom": "^3.0.5"
    }
}
