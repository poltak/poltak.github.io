{
    "name": "poltak.github.io",
    "version": "0.0.2",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "deploy:raw": "gh-pages -d build",
        "deploy": "npm run build && npm run deploy:raw",
        "test": "vitest",
        "test:single": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:ui": "vitest --ui",
        "lint": "prettier --check .",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^7.0.0",
        "@sveltejs/adapter-static": "^3.0.10",
        "@sveltejs/kit": "^2.50.2",
        "@sveltejs/vite-plugin-svelte": "^6.2.4",
        "@types/seedrandom": "^3.0.8",
        "@vitest/coverage-v8": "^4.0.18",
        "@vitest/ui": "^4.0.18",
        "@testing-library/svelte": "^5.2.5",
        "@testing-library/user-event": "^14.6.1",
        "gh-pages": "^6.3.0",
        "jsdom": "^28.0.0",
        "mdsvex": "^0.12.6",
        "prettier": "^3.8.1",
        "prettier-plugin-svelte": "^3.4.1",
        "svelte": "^5.49.2",
        "svelte-check": "^4.3.6",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3",
        "vite": "^7.3.1",
        "vitest": "^4.0.18"
    },
    "dependencies": {
        "kindle-highlights-parser": "^0.1.2",
        "minisearch": "^7.2.0",
        "poltak-epub-parser": "^0.1.1",
        "seedrandom": "^3.0.5"
    }
}
